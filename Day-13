#include <stdio.h>
int main() {
    int X, Y, A, B, t, m;
    printf("Enter the required power generated by project each year (in units) : ");
    scanf("%d", &A);
    
    printf("Enter the number of years : ");
    scanf("%d", &B);
    t = A * B;
    printf("Total amount of power needed by chefland is %d units\n",t);
    printf("Enter the amount of Helium-3 on moon (in grams) : ");
    scanf("%d", &X);
    
    printf("1 gram of Helium-3 provide power (in units) : ");
    scanf("%d", &Y);
    m = X * Y;
    printf("Power provided by Helium-3 present on moon is %d units\n",m);
    if(m>=t){
        printf("YES");
    }else{
        printf("NO");
    }
    return 0;
}
