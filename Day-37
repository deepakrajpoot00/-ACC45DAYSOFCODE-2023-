#include <stdio.h>
int main() {
    int A1,A2,B1,B2,a,b,c;
    printf("Enter the exports and imports goods worth of country A : ");
    scanf("%d %d", &A1, &A2);
    
    printf("Enter the exports and imports goods worth of country B : ");
    scanf("%d %d", &B1, &B2);
    a = A1 - A2;
    b = B1 - B2;
    c = a + b;
    printf("The net exports of country A and B is %d\n",c);
    if((-1*c)>0){
        printf("YES, the trade of country C is in trade surplus");
    }else{
        printf("NO, the trade of country C is not in trade surplus");
    }
    return 0;
}
